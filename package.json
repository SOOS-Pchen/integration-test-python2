{
  "name": "notes-wrapper",
  "version": "1.5.0",
  "private": true,
  "description": "A wrapper app for notes-engine.",
  "repository": "svn://scm.vertekdev.net/devrepo/platform/ui/notes-wrapper/...",
  "author": "llevit",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment production",
    "zip": "del /f dist\\%npm_package_name%-%npm_package_version%-SNAPSHOT.zip && 7z a dist\\%npm_package_name%-%npm_package_version%-SNAPSHOT.zip .\\dist\\*",
    "maven-install": "mvn install:install-file -DgroupId=com.vertek.platform.ui -DartifactId=%npm_package_name% -Dversion=%npm_package_version%-SNAPSHOT -Dpackaging=zip -Dfile=dist\\%npm_package_name%-%npm_package_version%-SNAPSHOT.zip",
    "build-zip-install": "npm run build && npm run zip && npm run maven-install",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start-windows-local": "rd /s/q dist tmp & ember server --port 4208 --proxy https://1910llevit.local:8443/oms/ --secure-proxy=false",
    "start-windows-remote": "rd /s/q dist tmp & ember server --port 4208 --proxy https://platform-branch.vertek.com/oms",
    "test": "ember test",
    "test:all": "ember try:each"
  },
